{
  "presets": [
    [
      "@parcel/babel-preset-env"
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
  ],
  "plugins": [
    "@parcel/babel-plugin-transform-runtime",
    "relay",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": false
      }
    ]
  ]
}